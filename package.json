{
  "name": "nsn-node",
  "version": "1.0.0",
  "description": "This is server to send push notifications with aws ",
  "main": "index.js",
  "scripts": {
    "dev": "tsnd --respawn src/app.ts",
    "build": "rimraf ./dist && tsc",
    "start": "node dist/app.js",
    "test": "NODE_ENV=test jest",
    "test:watch": "npm run docker:test && jest --watch",
    "test:coverage": "npm run docker:test && jest --coverage",
    "docker:test": "docker compose -f docker-compose.test.yaml --env-file .env.test up -d"
  },
  "keywords": [
    "node",
    "aws",
    "NSN",
    "notifications",
    "push"
  ],
  "author": "DurteBv",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^22.12.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "env-var": "^7.5.0",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "redis": "^4.7.0",
    "socket.io": "^4.8.1",
    "uuid": "^11.0.5"
  }
}